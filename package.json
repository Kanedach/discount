{
  "name": "sftoolApi",
  "version": "0.1.0",
  "description": "Backendtool for the sftool",
  "author": "Fernando Stutz",
  "license": "MIT",
  "scripts": {
    "copy": "cp package.json dist && cp manifest.yml dist && cp src/swagger.json dist",
    "deploy": "rm -rf dist/ && npm run build-prod && npm run push-iapc",
    "start": "nodemon",
    "start-prod": "node server.js",
    "build-prod": "npm run build && npm run copy",
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "push-iapc": "cf push -p dist -f dist/manifest.yml"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.17",
    "body-parser": "^1.19.0",
    "cfenv": "^1.2.2",
    "cheerio": "^1.0.0-rc.3",
